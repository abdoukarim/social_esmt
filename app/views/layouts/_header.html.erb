 <header>
        <%= link_to logo, root_path %>
        <nav class="round">
          <ul>
            <li><%= link_to "Accueil", root_path %></li>
            <% if signed_in? %>
            <li><%= link_to "Membres", users_path %></li>
            <li><%= link_to "Profile", current_user %></li>
            <li><%= link_to "Parametres", edit_user_path(current_user) %></li>
            <% end %>
            <li><%= link_to "Aide", help_path %></li>
            <% if signed_in? %>
            <li><%= link_to "Deconnexion", signout_path, :method => :delete %></li>
            <% else %>
            <li><%= link_to "Connexion", signin_path %></li>
            <% end %>
          </ul>
        </nav>
</header>